{
  "name": "flatcrawl",
  "version": "1.0.0",
  "description": "Application that opens browser with URLs and tracks them in CSV",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": "20.16.0",
    "pnpm": "7.33.6"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx index.ts",
    "sync-to-gist": "tsx syncToGist.ts",
    "start-and-sync": "pnpm run start && pnpm run sync-to-gist",
    "viewer": "node server.js"
  },
  "keywords": [
    "browser",
    "automation",
    "csv",
    "puppeteer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "csv-parser": "^3.2.0",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.3",
    "fs-extra": "^11.3.0",
    "puppeteer": "^24.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.28",
    "typescript": "^5.8.2"
  }
}
